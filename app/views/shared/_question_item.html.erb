<div class="question-item clearfix">
  <div class="row">  
    <div class="col-9">

        <h5><%= link_to question.title, question_path(question) %></h5>
        <p><%= truncate(question.description, length: 100) %></p>
        <%= time_ago_in_words question.created_at %> ago
     
    </div>
  </div>
</div>
